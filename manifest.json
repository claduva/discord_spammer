{
    "manifest_version": 2,
   "name": "Discord Clicker",
    "description": "This extension clicks in discord",
    "version": "1.0",
   "content_scripts": [
        {
        "matches": ["https://discord.com/*"],
        "js": ["jquery.js","script.js"]
        }
    ],
    
    "page_action": {
    "default_icon": "img.png",
    "default_popup": "popup.html",
    "default_title": "My custom google page!"
    }
   }